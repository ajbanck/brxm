{
  "name": "@bloomreach/navapp",
  "version": "1.1.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve navapp --public-host http://127.0.0.1:4200",
    "start:example-app": "ng serve iframe-app-example",
    "sprites": "svg-sprite --config sprite-config.json src/assets/menu-icons/*.svg",
    "build": "ng build",
    "test": "ng test",
    "test:single-run": "ng test --no-watch --browsers ChromeHeadless",
    "lint": "npm run lint:styles && ng lint",
    "lint:styles": "stylelint \"projects/**/*.scss\"",
    "build:prod": "ng build --prod",
    "prerelease:navapp": "npm run lint && npm run test:single-run && npm run build:prod",
    "release:navapp": "release-it",
    "release:navapp-communication": "cd ./projects/navapp-communication && release-it",
    "docs": "typedoc && minicat projects/navapp-communication/src/docs/typedoc.css >> docs/assets/css/main.css"
  },
  "browser": "dist/navapp/main.js",
  "files": [
    "dist/navapp/"
  ],
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@bloomreach/navapp-communication": "1.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "core-js": "^2.5.4",
    "ng-click-outside": "~4.0.0",
    "ngx-logger": "~4.0.7",
    "ngx-perfect-scrollbar": "~8.0.0",
    "normalize.css": "~8.0.1",
    "open-sans-fontface": "^1.4.0",
    "penpal": "^4.0.0",
    "rxjs": "~6.5.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "8.3.0",
    "@angular-builders/dev-server": "7.3.1",
    "@angular-devkit/build-angular": "~0.803.19",
    "@angular-devkit/build-ng-packagr": "~0.803.19",
    "@angular/cli": "~8.3.19",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "2.0.1",
    "minicat": "^1.0",
    "ng-packagr": "^5.4.0",
    "ngx-cookie-service": "^2.2.0",
    "protractor": "~5.4.0",
    "release-it": "^12.0.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.0",
    "stylelint-scss": "^3.6.1",
    "ts-node": "~7.0.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.15.0",
    "typescript": "~3.5.3",
    "webpack-assets-manifest": "^3.1.1"
  }
